#include <iostream>
using namespace std;

int main() {
	//code
	int t;cin>>t;
	while(t--)
	{
	    int n;cin>>n;
	    int arr[n];
	    int sum=0;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	        sum+=arr[i];
	    }
	    
	    if(sum%2!=0)
	      {  cout<<"NO\n";continue;}
	      sum=sum/2;
	    bool dp[n+1][sum+1];
	    for(int i=0;i<=n;i++)
	        dp[i][0]=1;
	    for(int j=0;j<=sum;j++)
	            dp[0][j]=0;
	    for(int i=1;i<=n;i++)
	    {
	        for(int j=1;j<=sum;j++)
	        {
	            if(arr[i-1]<=j)
	            {
	                dp[i][j]=dp[i-1][j] || dp[i-1][j-arr[i-1]];
	            }
	            else
	                dp[i][j]=dp[i-1][j];
	        }
	    }
	    if(dp[n][sum])
	        cout<<"YES\n";
	    else
	        cout<<"NO\n";
	    
	}
	return 0;
}
